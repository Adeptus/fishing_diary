= simple_form_for @expedition do |f|
  = f.input :start_at, as: :datetime_picker
  = f.input :end_at, as: :datetime_picker
  = f.input :place_id, collection: Place.all
  = f.input :notes
  = f.input :pressure
  = f.input :showers
  = f.input :overcast
  = f.input :temperature
  = f.button :submit, class: 'btn-primary'
  = link_to _("cancel"), expeditions_path, class: 'btn btn-default'
